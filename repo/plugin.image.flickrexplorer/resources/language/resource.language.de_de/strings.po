#: 
msgctxt "#30111" 
#: "This term is shown in Preferences for a listbox for setting the maximum number of items per page. "
msgid "maximum number of items per page"
msgstr "maximale Anzahl Objekte pro Seite"

#:
msgctxt "#30200" 
#: "Preferences Category Label"
msgid "Startup Settings"
msgstr "Startup-Einstellungen"

#: 
msgctxt "#30201" 
#: "Startup Preferences"
msgid "Turn debug logging on or off"
msgstr "Schaltet die Debug-Protokollierung ein oder aus"

msgctxt "#30501" 
#: "Cache Preferences"
msgid "Days until deletion"
msgstr "Löschfrist (Tage)"

msgctxt "#30502" 
#: "Preferences: max. slidestore"
msgid "max. storage size for slideshows"
msgstr "max. Speicher auf Datenträger für Slideshows"

msgctxt "#30101" 
#: "Category Label User"
msgid "Flickr: Username, Mail or User_ID"
msgstr "Flickr: Benutzername, Mail oder User_ID"

msgctxt "#30102" 
#: "Preferences: User info1"
msgid "Info 1: for Menu MyFlickr you have to set Username"
msgstr "Info 1: für das Menu MyFlickr ist ein Benutzername notwendig"

msgctxt "#30103" 
#: "Preferences: User info2"
msgid "Info 2:  Addon displays public content only (no login)"
msgstr "Info 2: das Addon zeigt nur öffentliche Inhalte (kein Login)"

msgctxt "#30104" 
#: "Preferences: User info3"
msgid "Info 3:  username can be a any Flickr user name"
msgstr "Info 3: Benutzername kann ein beliebiger Flickr Benutzername sein"

msgctxt "#30105" 
#: "Preferences: maxPageContent Info"
msgid "Info:  higher values increase the loading time for slideshows"
msgstr "Info: höhere Werte erhöhen Speicher + Ladezeit für Diashows"

msgctxt "#30112" 
#: "Preferences Sort method"
msgid "Sort method - only for Search"
msgstr "Sortiermethode - nur für das Menü Suche"

msgctxt "#30113" 
#: "Preferences: show videos"
msgid "show videos if available"
msgstr "Videos zeigen, falls verfügbar"

msgctxt "#30114" 
#: "Preferences: query for menu FlickrPeople"
msgid "search term for menu FlickrPeople"
msgstr "Suchbegriff für das Menü FlickrPeople"

msgctxt "#30115" 
#: "Preferences Category: Picture display"
msgid "Picture display"
msgstr "Bildanzeige"

