						<depth>DepthContentPopout</depth>
            <left>21</left>
						<top>45</top>
						<include content="SmallLandscapeLayout">
							<param name="focused" value="true" />
						</include>
						<include>LandscapeWallTimer</include>
						<include>SetDBTypeProperty</include>
					</control>
				</focusedlayout>
