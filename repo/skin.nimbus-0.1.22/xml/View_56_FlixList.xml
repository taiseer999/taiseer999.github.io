								<textcolor>$VAR[FocusedTextColorVar]</textcolor>
								<shadowcolor>text_shadow</shadowcolor>
								<visible>Container.Content(movies) | Container.Content(seasons) | Container.Content(episodes) | Container.Content(videos)</visible>
							</control>
							<control type="label">
								<right>150</right>
								<aligny>center</aligny>
								<scroll>true</scroll>
								<scrollspeed>160</scrollspeed>
								<font>font27</font>
