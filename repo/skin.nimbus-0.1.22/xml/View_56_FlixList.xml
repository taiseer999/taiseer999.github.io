							<label>$INFO[ListItem.Rating]</label>
							<textcolor>$VAR[FocusedTextColorVar]</textcolor>
							<visible>![$EXP[isMovie] | $EXP[isTVShow] | $EXP[isSeason]]</visible>
							<shadowcolor>text_shadow</shadowcolor>
						</control>
						<control type="label">
							<top>-2</top>
							<bottom>0</bottom>
							<right>20</right>
							<align>right</align>
							<aligny>center</aligny>
							<font>font27</font>
							<label>$INFO[ListItem.Property(WatchedEpisodes)]$INFO[ListItem.Property(TotalEpisodes), / ,]</label>
							<textcolor>$VAR[FocusedTextColorVar]</textcolor>
