                            <onup>SetFocus(8100,9999,absolute)</onup>
                            <ondown>SetFocus(8100,0,absolute)</ondown>
                            <!-- <include content="Settings_Label">
                                <param name="dialog">true</param>
                                <param name="id">8091</param>
                                <label>$LOCALIZE[587]</label>
                                <top>0</top>
                                <height>40</height>
                            </include> -->
                            <include content="Settings_Button">
                                <param name="dialog">true</param>
                                <param name="id">8001</param>
                                <param name="control">edit</param>
                                <texturenofocus colordiffuse="$VAR[Color_DialogBorder]" border="40">common/menu.png</texturenofocus>
                            </include>
                            <control type="group" id="8092">
                                <top>0</top>
                                <height>40</height>
                                <control type="image">
                                    <height>1</height>
                                    <left>20</left>
                                    <right>20</right>
                                    <centertop>50%</centertop>
                                    <texture colordiffuse="$VAR[Color_DialogBorder]">common/white.png</texture>
                                </control>
                            </control>
                            <include content="Settings_ButtonImage">
                                <param name="dialog">true</param>
                                <param name="id">8002</param>
                                <param name="groupid">8102</param>
                                <param name="control">button</param>
                                <param name="icon">special://skin/extras/icons/folder.png</param>
                                <param name="left">20</param>
                                <label>$LOCALIZE[1024]</label>
                                <onclick condition="!String.IsEmpty(Window.Property(url))">RunPlugin($INFO[Window.Property(url),,&amp;func=do_icon&amp;&amp;icon_dir::])</onclick>
                                <onclick condition="String.IsEmpty(Window.Property(url)) + !String.IsEmpty(Window.Property(setting))">Skin.SetImage($INFO[Window.Property(setting)])</onclick>
                                <texturenofocus />
                            </include>
                            <control type="group" id="8093">
                                <top>0</top>
                                <height>40</height>
                                <control type="image">
                                    <height>1</height>
                                    <left>20</left>
                                    <right>20</right>
                                    <centertop>50%</centertop>
                                    <texture colordiffuse="$VAR[Color_DialogBorder]">common/white.png</texture>
                                </control>
                            </control>
                        </control>
                        <include content="List_Dialog">
                            <param name="id">8100</param>
                            <param name="textcolor">dialog_fg</param>
                            <param name="misc">[!String.IsEmpty(Window.Property(SelectionList)) + Integer.IsEqual(ListItem.CurrentItem,Container(8100).CurrentItem)]</param>
                            <top>260</top>
                            <left>20</left>
                            <right>20</right>
                            <bottom>20</bottom>
                            <onright>450</onright>
                            <ondown>ClearProperty(SelectionList)</ondown>
                            <ondown>8001</ondown>
                            <onup>ClearProperty(SelectionList)</onup>
                            <onup>8002</onup>
                            <onfocus>SetProperty(SelectionList,1)</onfocus>
                            <content>
                                <item>
                                    <label>$LOCALIZE[31382]</label>
                                </item>
                                <item>
                                    <label>Standard</label>
                                    <property name="filter">&amp;filter_key=label&amp;filter_value=film / tv / music / song / image.png / images.png / heart.png / youtube / netflix / disney / compass / cricket / ball / gem / rocket / diamond / director / dove / dragon / drum / umbrella / dungeon / fire / fish / octopus / flask / frog / mask / game / ghost / genre / groove / guitar / gun / hammer / hippo / horse / house.png / hotel.png / hotdog / hot-tub / igloo / icons.png / instagram / jedi / kiwi / key / leaf / meteor / microphone / radio / mosquito / motorcycle / otter / paint / paw.png / pepper / pizza / reddit / robot / santa / sailboat / soccer / spotify / splotch / steam / supernes / tennis / worm / beer / binoculars / bicycle / biohazard / bolt / bone / brain / brush / bucket / burger / burst / cake / candy / cat.png / champagne / chair / clapperboard / construction / controlpanel / couch / cow.png / crow.png / crown / crunchyroll / cube.png / dog.png / favourites.png / fingerprint / gift.png / golf.png / grooveshark / kodi / lightbulb / podcast / power.png / scrissors / screwdriver / seedling / setting / shrimp / shuttle / skull / sink.png / sleigh / spider / spaghetti / sun2 / tape.png / target.png / vials / volcano / world.png / search.png</property>
                                </item>
                                <item>
                                    <label>Arrow</label>
                                    <property name="filter">&amp;filter_key=label&amp;filter_value=arrow</property>
                                </item>
                                <item>
                                    <label>Shapes</label>
                                    <property name="filter">&amp;filter_key=label&amp;filter_value=circle / square / rectangle / shape / box</property>
                                </item>
                                <item>
                                    <label>File</label>
                                    <property name="filter">&amp;filter_key=label&amp;filter_value=file / folder</property>
                                </item>
                                <item>
                                    <label>Faces</label>
                                    <property name="filter">&amp;filter_key=label&amp;filter_value=face</property>
                                </item>
                                <item>
                                    <label>Gestures</label>
                                    <property name="filter">&amp;filter_key=label&amp;filter_value=hand</property>
                                </item>
                                <item>
                                    <label>User</label>
                                    <property name="filter">&amp;filter_key=label&amp;filter_value=person / user</property>
                                </item>
                            </content>
                        </include>
                    </control>
                </control>

                <!-- Right -->
                <control type="group">
                    <left>480</left>
                    <include>Part_Panel</include>
                    <include content="List_IconBrowser">
                        <param name="id">450</param>
                        <param name="item_h">150</param>
                        <param name="itemlayout_h">190</param>
                        <param name="item_w">210</param>
                        <param name="itemlayout_w">250</param>
                        <param name="detailed">true</param>
                        <param name="textcolor">dialog_fg</param>
                        <onleft>8100</onleft>
                        <onright>61</onright>
                        <pagecontrol>61</pagecontrol>
                        <content>plugin://script.skinvariables/?info=get_container_labels&amp;containers=451&amp;infolabel=Label&amp;thumb=PicturePath&amp;reload=$INFO[Container(451).NumItems]$VAR[Path_IconSelector_Filter]</content>
                        <onclick condition="!String.IsEmpty(Window.Property(url))">RunPlugin($INFO[Window.Property(url),,&amp;func=do_icon]$INFO[Container(450).ListItem.Icon,&amp;&amp;value::,])</onclick>
                        <onclick condition="String.IsEmpty(Window.Property(url)) + !String.IsEmpty(Window.Property(setting))">Skin.SetString($INFO[Window.Property(setting)],$INFO[Container(450).ListItem.Icon])</onclick>
                        <onclick>Close</onclick>
                    </include>
                </control>

                <include content="Part_Scrollbar">
                    <onleft>450</onleft>
                    <top>40</top>
                    <bottom>40</bottom>
                </include>
            </control>


        </control>

        <include content="Furniture_Busy">
            <param name="hasspinner">true</param>
            <param name="spinspinner">true</param>
            <param name="icon">special://skin/extras/icons/kodi.png</param>
            <param name="haslabel">false</param>
            <param name="spinnervisible">[!Window.IsActive(DialogVolumeBar.xml) | Player.Muted]</param>
            <param name="visible_delay">400</param>
            <param name="windowopen_condition">False</param>
            <param name="visible">[Container(450).IsUpdating]</param>
        </include>

    </controls>
</window>
