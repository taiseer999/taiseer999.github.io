                <texture colordiffuse="$PARAM[colordiffuse_off]">buttons/check-off.png</texture>
                <width>28</width>
                <height>28</height>
                <visible>!$PARAM[selected]</visible>
            </control>
            <control type="image">
                <centerleft>0</centerleft>
                <centertop>50%</centertop>
                <texture colordiffuse="$PARAM[colordiffuse_on]">buttons/check-on.png</texture>
