    <include name="Part_List_Icon">
        <param name="icon">$INFO[ListItem.Icon]</param>
        <param name="is_defaulticon">[String.StartsWith(ListItem.Icon,Default) | String.StartsWith(ListItem.Icon,special://skin/extras/icons/)]</param>
        <param name="colordiffuse">dialog_fg_90</param>
        <definition>
            <control type="group">
                <left>0</left>
                <width>$PARAM[item_h]</width>
                <control type="group">
                    <left>0</left>
                    <top>20</top>
                    <bottom>20</bottom>
                    <right>40</right>
                    <control type="image">
                        <aspectratio>keep</aspectratio>
                        <texture background="true" fallback="$PARAM[fallback]" diffuse="diffuse/square_w120_h120.png">$PARAM[icon]</texture>
                        <visible>![$PARAM[is_defaulticon]]</visible>
                    </control>
                    <control type="image">
                        <aspectratio>keep</aspectratio>
                        <texture background="true" fallback="$PARAM[fallback]" colordiffuse="$PARAM[colordiffuse]" diffuse="diffuse/square_w120_h120.png">$PARAM[icon]</texture>
                        <visible>$PARAM[is_defaulticon]</visible>
                    </control>
                </control>
            </control>
        </definition>
    </include>

