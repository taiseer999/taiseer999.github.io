<?xml version="1.0" encoding="UTF-8"?>
<window>

    <defaultcontrol>450</defaultcontrol>
    <controls>

        <include content="Object_Hidden_Button">
            <param name="id">413</param>
        </include>
        <include content="Object_Hidden_Button">
            <param name="id">414</param>
        </include>
        <include content="Object_Hidden_Button">
            <param name="id">415</param>
        </include>
        <include content="Object_Hidden_Button">
            <param name="id">416</param>
        </include>

        <include>Dialog_Dim_Overlay</include>

        <control type="group">
            <visible>!Skin.HasSetting(Dialog.DisableArtworkPreview)</visible>
            <include>Animation_Right_Delay</include>
            <control type="group">
                <include content="Dialog_Preview_Def">
                    <param name="icon">$INFO[Container(451).ListItem.Icon]</param>
                </include>
                <visible>Control.IsVisible(451)</visible>
                <visible>String.IsEqual(Control.GetLabel(411),$LOCALIZE[13511]) | String.IsEqual(Control.GetLabel(411),$LOCALIZE[20437])</visible>
                <animation effect="slide" end="0,200" condition="Container(451).Row(2) | Container(451).Row(3)">Conditional</animation>
            </control>
        </control>

        <control type="group">
            <include>Animation_Right_Delay</include>
            <control type="group">
                <include>Dimension_DialogWideMenu</include>


                <include content="Dialog_Foreground_Side">
                    <param name="dialog_size">Dimension_DialogWideMenu</param>
                    <param name="subline">false</param>
                </include>

                <control type="image">
                    <bottom>160</bottom>
                    <height>1</height>
                    <left>80</left>
                    <right>80</right>
                    <texture colordiffuse="$VAR[Color_DialogBorder]">common/white.png</texture>
                    <visible>Control.IsVisible(450)</visible>
                </control>
                <control type="image">
                    <bottom>80</bottom>
                    <height>1</height>
                    <left>80</left>
                    <right>80</right>
                    <texture colordiffuse="$VAR[Color_DialogBorder]">common/white.png</texture>
                    <visible>!Control.IsVisible(450)</visible>
                </control>

                <control type="group">

                    <!-- Header -->
                    <include content="Dialog_Head">
                        <param name="id">411</param>
                    </include>
                    <include content="Dialog_Head">
                        <param name="id">9901</param>
                        <param name="visible">String.IsEmpty(Control.GetLabel(411))</param>
                        <param name="label">$LOCALIZE[424]</param>
                    </include>
                    <include content="Dialog_Manage_Head_Button">
                        <param name="onclick">SetFocus(9898)</param>
                    </include>

                    <!-- Content -->
                    <control type="group">
                        <include>Dimension_Dialog_Body</include>
                        <control type="group">
                            <include>Dimension_Dialog_Body_Group</include>
                            <include content="List_DialogSelect">
                                <param name="id">450</param>
                                <param name="detailed">True</param>
                                <param name="item_height">90</param>
                                <param name="textcolor">dialog_fg</param>
                                <pagecontrol>61</pagecontrol>
                                <onright>61</onright>
                                <bottom>80</bottom>
                                <ondown>9000</ondown>
                                <onup>9000</onup>
                            </include>

                            <include content="List_IconBrowser">
                                <param name="id">451</param>
                                <param name="item_h">160</param>
                                <param name="itemlayout_h">200</param>
                                <param name="item_w">180</param>
                                <param name="itemlayout_w">220</param>
                                <param name="detailed">true</param>
                                <param name="textcolor">dialog_fg</param>
                                <top>-20</top>
                                <bottom>-20</bottom>
                                <left>-20</left>
                                <right>-20</right>
                                <onright>62</onright>
                                <pagecontrol>62</pagecontrol>
                            </include>
                        </control>
                        <include content="Dialog_Scrollbar">
                            <param name="id">61</param>
                            <visible>Control.IsVisible(450)</visible>
                            <onleft>450</onleft>
                            <onright>9898</onright>
                            <bottom>100</bottom>
                        </include>
                        <include content="Dialog_Scrollbar">
                            <param name="id">62</param>
                            <visible>Control.IsVisible(451)</visible>
                            <onleft>451</onleft>
                            <onright>9898</onright>
                        </include>
                    </control>
                    <control type="group">
                        <visible>Control.IsVisible(450)</visible>
                        <control type="label" id="412">
                            <font>font_mini</font>
                            <textcolor>dialog_fg_70</textcolor>
                            <left>80</left>
                            <bottom>40</bottom>
                            <height>120</height>
                            <right>480</right>
                            <haspath>true</haspath>
                        </control>
                        <include content="Dialog_Tray_Grouplist">
                            <param name="onup">noop</param>
                            <onup>SetFocus(450,99999,absolute)</onup>
                            <ondown>SetFocus(450,0,absolute)</ondown>
                            <include content="Dialog_Button_Expanded">
                                <param name="id">9001</param>
                                <param name="icon">special://skin/extras/icons/circle-check.png</param>
                                <param name="label">$LOCALIZE[31032]</param>
                                <param name="visible">Control.IsEnabled(413) + Control.IsVisible(413) + Control.IsVisible(450)</param>
                                <onclick>SendClick(413)</onclick>
                            </include>
                            <include content="Dialog_Button_Expanded">
                                <param name="id">9002</param>
                                <param name="icon">special://skin/extras/icons/circle-xmark.png</param>
                                <param name="label">$LOCALIZE[222]</param>
                                <param name="visible">Control.IsEnabled(414) + Control.IsVisible(414)</param>
                                <onclick>SendClick(414)</onclick>
                            </include>
                        </include>
                    </control>
                </control>
            </control>
        </control>
        <include content="Object_Hidden_Button">
            <param name="id">64</param>
            <onfocus condition="Control.IsVisible(450)">SetFocus(450)</onfocus>
            <onfocus condition="Control.IsVisible(451)">SetFocus(451)</onfocus>
        </include>

        <include content="Dialog_Manage">
            <param name="onback">64</param>
