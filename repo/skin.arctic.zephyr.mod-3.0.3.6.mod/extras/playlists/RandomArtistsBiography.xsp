<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="artists">
    <name>Random Artists with Biography</name>
    <match>all</match>
    <rule field="biography" operator="greaterthan">
        <value>1</value>
    </rule>
    <rule field="biography" operator="true" />
    <order direction="ascending">random</order>
</smartplaylist>
